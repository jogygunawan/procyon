<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/berandaFragment">

    <fragment
        android:id="@+id/berandaFragment"
        android:name="com.example.projectmassive.fragment.BerandaFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_beranda" >
        <action
            android:id="@+id/action_homeFragment_to_activityFragment"
            app:destination="@id/activityFragment" />
    </fragment>
    <fragment
        android:id="@+id/catatanFragment"
        android:name="com.example.projectmassive.fragment.HomeFragment"
        android:label="fragment_catatan"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/profilFragment"
        android:name="com.example.projectmassive.fragment.ProfilFragment"
        android:label="fragment_profil"
        tools:layout="@layout/fragment_profil" />
    <dialog
        android:id="@+id/toDoDialogFragment"
        android:name="com.example.projectmassive.fragment.ToDoDialogFragment"
        android:label="fragment_to_do_dialog"
        tools:layout="@layout/fragment_to_do_dialog" />
    <fragment
        android:id="@+id/activityFragment"
        android:name="com.example.projectmassive.fragment.ActivityFragment"
        android:label="fragment_activity"
        tools:layout="@layout/fragment_activity" />
</navigation>